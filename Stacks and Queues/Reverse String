public class Solution {
    public String reverseString(String A) {
        Stack<Character> s = new Stack<>();
        for (int i =0 ; i < A.length(); i++) {
            s.push(A.charAt(i));
        }
        String ans = "";
        while (s.size() > 0) {
            ans += s.pop();
        }
        return ans;
    }
}
